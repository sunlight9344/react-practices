{
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                // ie 이놈 때문에 es 버전이 많이 내려간다
                // "ie": 11, 
                "edge": "89",
                "firefox": "92",
                "chrome": "90",
                "opera": "76",
                "safari": "15"
            }
        }],
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-syntax-throw-expressions"
    ]
}